# load_module /usr/lib64/nginx/modules/ngx_stream_module.so;
user  LBR;                   #表示当系统在执行worker进程的时候由哪个用户去执行，（默认为nobody）
worker_processes  1;            #逻辑CPU的个数设置的值为：（n-1）
error_log /home/LBR/error.log;
pid /home/LBR/nginx.pid;

events {
    worker_connections 768;
}

stream {
    # server {
    #     listen 50658;
    #     proxy_pass 62.3.54.31:12201;
    # }

    server {
        listen 12201;
        proxy_pass ss.jpnlink.xyz:12201;
    }
}

 
