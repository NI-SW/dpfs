dprofilePath=$(history -w /dev/stdout | awk 'END {print $2}')
dpfsPath=$(dirname $(readlink -f $dprofilePath))

echo dprofilePath=$dprofilePath
echo dpfsPath=$dpfsPath

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:\
$dpfsPath/thirdparty/spdk/build/lib:\
$dpfsPath/thirdparty/spdk/dpdk/build/lib:\
$dpfsPath/lib

# unset LD_LIBRARY_PATH
